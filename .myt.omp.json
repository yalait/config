{
  "final_space": true,
  "console_title": true,
  "console_title_style": "folder",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "horizontal_offset": 0,
      "vertical_offset": 0,
      "segments": [
        {
          "type": "path",
          "style": "diamond",
          "powerline_symbol": "",
          "invert_powerline": false,
          "foreground": "#ffffff",
          "background": "#25BA0E",
          "leading_diamond": "",
          "trailing_diamond": "",
          "properties": {
            "prefix": "  ",
            "style": "folder"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "",
          "invert_powerline": false,
          "foreground": "#193549",
          "background": "#F55919",
          "leading_diamond": "",
          "trailing_diamond": "",
          "properties": {
            "display_status": true,
            "display_stash_count": true,
            "display_upstream_icon": true
          }
        },
        {
          "type": "kubectl",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#000000",
          "background": "#2F55B2",
          "properties": {
            "prefix": " \uFD31 ",
            "template": "{{.Context}}{{if .Namespace}} >> {{.Namespace}}{{end}}"
          }
        },
        {
          "type": "terraform",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#000000",
          "background": "#8A61D3",
          "properties": {
            "template": "{{.WorkspaceName}}"
          }
        },
        {
          "type": "python",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#100e23",
          "background": "#906cff",
          "properties": {
            "prefix": " \uE235 "
          }
        },
        {
          "type": "root",
          "style": "powerline",
          "powerline_symbol": "",
          "invert_powerline": false,
          "foreground": "#ffffff",
          "background": "#0089e5",
          "leading_diamond": "",
          "trailing_diamond": "",
          "properties": {
            "root_icon": "\uF0E7"
          }
        },
        {
          "type": "exit",
          "style": "powerline",
          "powerline_symbol": "",
          "invert_powerline": false,
          "foreground": "#ffffff",
          "background": "#2e9599",
          "leading_diamond": "",
          "trailing_diamond": "",
          "properties": {
            "always_enabled": true,
            "color_background": true,
            "display_exit_code": false,
            "error_color": "#f1184c",
            "prefix": " "
          }
        }
      ]
    }
  ]
}
