[wsl2]
memory=6GB # Ограничиваем память для WSL2 VM.
processors=5 # Ограничиваем количество процессов для WSL2 VM.
localhostForwarding=true # Логическое значение, определяющее, должны ли порты, привязанные к подстановочному знаку или локальному хосту в виртуальной машине WSL 2, подключаться с хоста через localhost: port
nestedVirtualization=true # включить или выключить вложенную виртуализацию для WSL2
vmIdleTimeout=60000 # Количество миллисекунд, в течение которых виртуальная машина простаивает, прежде чем она будет отключена.
